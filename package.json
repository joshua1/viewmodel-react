{
  "name": "viewmodel-react",
  "version": "1.0.3",
  "description": "ViewModel for React",
  "main": "dist/viewmodel.js",
  "author": "Manuel De Leon <manuel@me.com> (https://viewmodel.org/)",
  "license": "MIT",
  "scripts": {
    "prepublishX": "babel --presets es2015 --out-dir dist src"
  },
  "peerDependencies": {
    "inferno-compat": "1.0.0-beta9"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "."
          ],
          "alias": {
            "react": "inferno-compat",
            "react-dom": "inferno-compat"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^2.4.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0"
  }
}
